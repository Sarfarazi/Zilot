                <path opacity="0.54"
                    d="M15.0001 18.75C18.4032 18.75 21.3514 20.4159 22.7984 22.8477C22.856 22.9444 22.8382 23.0677 22.7567 23.1453C20.7384 25.0681 18.0078 26.25 15.0001 26.25C11.9923 26.25 9.26178 25.0681 7.24341 23.1453C7.1619 23.0677 7.14418 22.9444 7.20174 22.8477C8.64868 20.4159 11.5969 18.75 15.0001 18.75Z"
                    fill="#222222" />
            </svg>
        </div>
    </header>

    
            <div class="fixed w-1/2 max-w-96 backdrop-blur-xl lg:top-18 z-20 p-3 lg:pt-5 left-5 md:left-10 lg:left-20 rounded-b-lg hidden searchBox">
             <input type="search" class="bg-white outline-0 border-0 py-3 px-5 w-full rounded-lg" name="search" id="search" placeholder="جستجو محصول...">

             <div class="bg-white p-2 pt-0 w-full rounded-lg mt-1 max-h-50 overflow-y-auto shadow-lg" id="searchResult">
              <p class="p-3 bg-gray-50 hover:bg-pastelGreen rounded-md mt-2 cursor-pointer">مینیمال-سکوت</p>
              <p class="p-3 bg-gray-50 hover:bg-pastelGreen rounded-md mt-2 cursor-pointer">مینیمال-آرام</p>
             </div>
            </div>


