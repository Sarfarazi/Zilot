    @RenderBody()

    <footer class="bg-lightGreen px-5 md:px-10 lg:px-20 pb-10 text-white relative">
        <div class="absolute bg-glassyWhite p-6 flex items-center justify-center z-10 left-5 md:left-10 lg:left-20 -translate-y-1/3 scrollBtn cursor-pointer"
            onclick="scrollToTop()">
            <img src="~/images/icons/arrow.svg" alt="arrow" />
        </div>
        <div class="flex flex-col lg:flex-row lg:items-end justify-between">
            <div>
                <div class="py-10 pb-2 w-fit patternContainer">
                    <p class="text-2xl md:text-3xl lg:text-4xl p-5">
                        زمیــن بـــرای زنــدگی
                    </p>
                </div>
                <div class="flex flex-col text-sm gap-3 p-5">
                    <a href="#">فرصت‌های شغلی</a>
                    <a href="#">پاسخ به پرسش‌های متداول</a>
                    <a href="#">رضایت مشتری و پیشنهادات</a>
                    <div class="text-base flex gap-5">
                        <span>تلفـن پشـتیـبانی</span>
                        <a href="tel:0211234567">021-1234567</a>
                    </div>
                </div>
            </div>

